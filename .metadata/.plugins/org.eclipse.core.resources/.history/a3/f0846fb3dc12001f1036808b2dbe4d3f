package javaPrograms;

public class amstromgNumber {
	
	
	static int rem=0, sum =0;
	
	public static void checkAmstrongNumber(int number)
	
	{
		
		while(number!=0)
			
		{
		
		rem= number%10;
		
		sum= sum +rem*rem*rem;
		
		number=number/10;
	}
	
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
